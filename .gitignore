# Global ignores
node_modules/
*.log
*.log.*
*.swp
*.tmp
*.temp
*.tsbuildinfo
.DS_Store
Thumbs.db
.env
.env*
# Exception: Allow a template file for reference
!.env.example
.vscode/
.idea/

# Ignore Docker-related files
docker-compose.override.yml
docker-compose.override.yaml
.dockerignore

# Ignore everything inside `frontend/node_modules` and `backend/node_modules`
/frontend/node_modules/
/backend/node_modules/

# Ignore build and dist outputs
/frontend/build/
/backend/dist/

/frontend/.eslintcache
/backend/.eslintcache

# Ignore environment files for both frontend and backend
/frontend/.env
/frontend/.env.*.local
/backend/.env
/backend/.env.*.local

# Ignore specific caches and coverage folders
/frontend/.parcel-cache/
/backend/logs/
coverage/
notes/
