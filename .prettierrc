{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": [
    "prettier-plugin-organize-imports", 
    "prettier-plugin-tailwindcss", 
    "prettier-plugin-packagejson", 
    "prettier-plugin-sort-json"
  ]
}